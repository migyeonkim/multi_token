{
  "global": {
    "basdSpace": {
      "$type": "spacing",
      "$value": "4"
    },
    "space-1": {
      "$type": "spacing",
      "$value": "{basdSpace}"
    },
    "space-2": {
      "$type": "spacing",
      "$value": "{basdSpace} * 2"
    },
    "space-3": {
      "$type": "spacing",
      "$value": "{basdSpace} * 3"
    },
    "space-4": {
      "$type": "spacing",
      "$value": "{basdSpace} * 4"
    },
    "space-5": {
      "$type": "spacing",
      "$value": "{basdSpace} * 5"
    },
    "space-6": {
      "$type": "spacing",
      "$value": "{basdSpace} * 6"
    },
    "orange": {
      "$type": "color",
      "$value": "#ff9d3f"
    },
    "white": {
      "$type": "color",
      "$value": "#fff"
    },
    "black": {
      "$type": "color",
      "$value": "#000"
    },
    "blue": {
      "$type": "color",
      "$value": "#536eff"
    },
    "radius-L": {
      "$type": "borderRadius",
      "$value": "8"
    },
    "base": {
      "$type": "fontSizes",
      "$value": "1.333"
    },
    "root": {
      "$type": "fontSizes",
      "$value": "14"
    },
    "Small": {
      "$type": "fontSizes",
      "$value": "{Medium} / {base}"
    },
    "Medium": {
      "$type": "fontSizes",
      "$value": "{root}"
    },
    "Large": {
      "$type": "fontSizes",
      "$value": "{Medium} * {base}"
    },
    "XL": {
      "$type": "fontSizes",
      "$value": "{Large} * {base}"
    }
  },
  "light": {
    "Secondary": {
      "$type": "color",
      "$value": "{black}"
    }
  },
  "dark": {
    "Secondary": {
      "$type": "color",
      "$value": "{white}"
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global",
      "light",
      "dark"
    ]
  }
}